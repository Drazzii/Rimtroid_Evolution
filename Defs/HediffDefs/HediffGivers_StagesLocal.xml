<?xml version="1.0" encoding="utf-8" ?>
<Defs>


<!--The following dictates which age changes the 'Mitosis' hediff to the next stage-->

<HediffGiverSetDef>
  <defName>RT_EvoGrowth_Local</defName>


        <li Class="RT_Core.HediffGiver_AfterPeriod">
          <!--<maxReroll>5</maxReroll>-->
          <chance>1</chance>
          <yearsInterval>1</yearsInterval>
          <hediffsToPreventGrowth>
              <li>RT_StuntedGrowthBad</li>
              <li>RT_StuntedGrowthGood</li>
              <li>RT_StuntedGrowthNeutral</li>
          </hediffsToPreventGrowth >
          <possibleEvolutionPaths>
              <li>
                <requiredAge>0</requiredAge>
                <hediff>RT_RimtroidHatchlingState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>1</weight>
              </li>
          </possibleEvolutionPaths>
        </li>



        <li Class="RT_Core.HediffGiver_AfterPeriod">
          <!--<maxReroll>5</maxReroll>-->
          <chance>1</chance>
          <yearsInterval>1</yearsInterval>
          <hediffsToPreventGrowth>
              <li>RT_StuntedGrowthBad</li>
              <li>RT_StuntedGrowthGood</li>
              <li>RT_StuntedGrowthNeutral</li>
          </hediffsToPreventGrowth >
          <possibleEvolutionPaths>
              <li>
                <requiredAge>14</requiredAge>
                <hediff>RT_RimtroidState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>1</weight>
              </li>
          </possibleEvolutionPaths>
        </li>



        <li Class="RT_Core.HediffGiver_AfterPeriod">
          <maxReroll>15</maxReroll>
          <chance>0.10</chance>
          <yearsInterval>3~9</yearsInterval>
          <hediffsToPreventGrowth>
              <li>RT_StuntedGrowthBad</li>
              <li>RT_StuntedGrowthGood</li>
              <li>RT_StuntedGrowthNeutral</li>
          </hediffsToPreventGrowth >
          <possibleEvolutionPaths>
              <li>
                <requiredAge>22</requiredAge>
                <hediff>RT_StalkerState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>1</weight>
              </li>
              <li>
                <requiredAge>26</requiredAge>
                <hediff>RT_CollosusState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>1</weight>
              </li>
              <li>
                <requiredAge>32</requiredAge>
                <hediff>RT_SeekerState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>1</weight>
              </li>
              <li>
                <requiredAge>36</requiredAge>
                <hediff>RT_BorealState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>1</weight>
              </li>
              <li>
                <requiredAge>42</requiredAge>
                <hediff>RT_PsitroidState</hediff>
                <partsToAffect>
                    <li>RT_Mitosis</li>
                </partsToAffect>
                <weight>0.75</weight>
              </li>
          </possibleEvolutionPaths>
        </li>




