<?xml version="1.0" encoding="utf-8" ?>
<Defs>


<!--The following dictates which age changes the 'Mitosis' hediff to the next stage-->

<HediffGiverSetDef>
  <defName>RT_EvoGrowth_Misc</defName>
  <hediffGivers>

    <li Class="RT_Core.HediffGiver_AfterPeriod">
      <stuntedHediffDef>RT_StuntedGrowthBad</stuntedHediffDef>
      <hediff>RT_ThetaState</hediff>
			<partsToAffect>
        <li>RT_Mitosis</li>
      </partsToAffect>
		<!--<targetAgeRange>0.00001~6.99999</targetAgeRange>-->
    </li>

    <li Class="RT_Core.HediffGiver_AfterPeriod">
      <stuntedHediffDef>RT_StuntedGrowthBad</stuntedHediffDef>
      <hediff>RT_MochtroidState</hediff>
			<partsToAffect>
        <li>RT_Mitosis</li>
      </partsToAffect>
    </li>

    <li Class="RT_Core.HediffGiver_AfterPeriod">
      <stuntedHediffDef>RT_StuntedGrowthBad</stuntedHediffDef>
      <hediff>RT_MegaroidState</hediff>
			<partsToAffect>
        <li>RT_Mitosis</li>
      </partsToAffect>
    </li>

    <li Class="RT_Core.HediffGiver_AfterPeriod">
      <stuntedHediffDef>RT_StuntedGrowthBad</stuntedHediffDef>
      <hediff>RT_DarkTallonState</hediff>
			<partsToAffect>
        <li>RT_Mitosis</li>
      </partsToAffect>
    </li>

    <li Class="RT_Core.HediffGiver_AfterPeriod">
      <stuntedHediffDef>RT_StuntedGrowthBad</stuntedHediffDef>
      <hediff>RT_ForsakenState</hediff>
			<partsToAffect>
        <li>RT_Mitosis</li>
      </partsToAffect>
    </li>

  </hediffGivers>
</HediffGiverSetDef>

</Defs>